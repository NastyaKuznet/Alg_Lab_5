<Page x:Class="Alg_Lab_5.V.FolderCreateNewGraph.CreateFinalyGraphP"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Alg_Lab_5.V.FolderCreateNewGraph"
      mc:Ignorable="d" 
      d:DesignHeight="200" d:DesignWidth="300"
      Title="CreateFinalyGraphP">
    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="TextNamGraph" Grid.Row="0" Margin="10" Text="Название графа" VerticalAlignment="Bottom"/>
        <TextBlock x:Name="NameGraph" Grid.Row="1" Margin="10" Text="{Binding NameGraph}" VerticalAlignment="Bottom"/>
        <TextBlock x:Name="TextPathGraph" Grid.Row="2" Margin="10" Text="Путь папки графа" VerticalAlignment="Bottom"/>
        <TextBlock x:Name="PathGraph" Grid.Row="3" Margin="10" Text="{Binding PathFolder}" VerticalAlignment="Bottom"/>
    </Grid>
</Page>
