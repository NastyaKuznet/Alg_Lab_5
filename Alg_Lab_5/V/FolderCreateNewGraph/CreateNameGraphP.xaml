<Page x:Class="Alg_Lab_5.V.FolderCreateNewGraph.CreateNameGraphP"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Alg_Lab_5.V.FolderCreateNewGraph"
      mc:Ignorable="d" 
      d:DesignHeight="200" d:DesignWidth="400"
      Title="CreateNameGraphP">
    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="TextNameUniverse" Grid.Column="0" Grid.Row="1" Text="Название графа" Margin="10" VerticalAlignment="Bottom"/>
        <TextBox x:Name="NameUniverse" Grid.Column="0" Grid.Row="2" Margin="10" BorderBrush="Blue" Text="{Binding NameGraph}"/>
    </Grid>
</Page>
